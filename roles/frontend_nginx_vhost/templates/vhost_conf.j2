
server {
        listen       {{ http_port }} ;
        server_name  {{ domain_name }};

          root {{ app_path }};    
location /
        {

                   try_files $uri $uri/ /index.html;

                access_log /var/log/nginx/{{ domain_name }}.access.error;
                error_log /var/log/nginx/{{ domain_name }}.error ;
        }
}

